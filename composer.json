{
    "name": "socialdept/atp-cbor",
    "description": "CBOR, CAR, and CID binary encoding for AT Protocol in PHP",
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/socialdept/atp-cbor",
    "keywords": [
        "Laravel",
        "AT Protocol",
        "ATP",
        "CBOR",
        "CAR",
        "CID",
        "Encoder",
        "Decoder",
        "Firehose",
        "Binary"
    ],
    "require": {
        "php": "^8.2",
        "ext-gmp": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "friendsofphp/php-cs-fixer": "^3.89"
    },
    "autoload": {
        "psr-4": {
            "SocialDept\\AtpCbor\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SocialDept\\AtpCbor\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    }
}
